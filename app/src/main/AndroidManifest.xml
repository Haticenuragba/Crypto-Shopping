<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crypto_shopping.cryptoshopping">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_myicon"
        android:label="@string/appname"
        android:roundIcon="@mipmap/ic_myicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".MainActivity"
            android:label="" />
        <activity android:name=".ProductDetailActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".OrderInfo" />
        <activity android:name=".SelectAddressActivity" />
        <activity android:name=".FinishPaymentActivity"></activity>
    </application>

</manifest>